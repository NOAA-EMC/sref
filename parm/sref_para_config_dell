#######################################################
# This file defines the various input varibles for the
# SREF model run
#######################################################
#
# machine=g
# machine=t
  machine=dell2

  export NWROOT=/gpfs/${machine}/nco/ops/nwprod
  export NWROOTp1=/gpfs/${machine}/nco/ops/nwprod
  export COMROOT=/gpfs/${machine}/nco/ops/com
  export COMROOTp1=/gpfs/${machine}/nco/ops/com
  export GESROOT=/gpfs/${machine}/nco/ops/nwges
  export GESROOTp1=/gpfs/${machine}/nco/ops/nwges

  export HOMEsref=/gpfs/dell2/emc/modeling/noscrub/${USER}/sref.v7_dell
  export EXECsref=$HOMEsref/exec
  export FIXsref=$HOMEsref/fix
  export PARMsref=$HOMEsref/parm
  export USHsref=$HOMEsref/ush
  export SCRIPTSsref=$HOMEsref/scripts
  export POSTGPSH=$HOMEsref/ush/global_postgp.sh

  export HOMEglobal=${NWROOT}
  export EXECglobal=$HOMEglobal/exec
  export FIXglobal=$HOMEglobal/fix
  export PARMglobal=$HOMEglobal/parm
  export USHglobal=$HOMEglobal/ush

  export SENDCOM=YES
  export SENDCOM_GB2=YES
  export SENDDBN=NO
  export SENDDBN_GB2=NO
  export SENDSMS=NO
  export DBNROOT=DBNROOT_OFF

  export RUN_TCTRACK=NO

  PTMP=/gpfs/dell2/ptmp
  export DATA_IN=$PTMP/$LOGNAME/tmpnwprd
  export jlogfile=$DATA_IN/jlogfile

  export com=$PTMP/$LOGNAME/com/sref/${envir}
  export COM=$PTMP/$LOGNAME/com/sref/${envir}
# export com=/com/sref/${envir}
# export COM=/com/sref/${envir}

  export COMglb_IN=/gpfs/${machine}/nco/ops/com/sref/${envir}
# export COMglb_IN=$PTMP/$LOGNAME/com/sref/${envir}
  export COM_IN=$PTMP/$LOGNAME/com/sref/${envir}
#
# ---------------------------------------
  export COM_OUT=$PTMP/$LOGNAME/com/sref/${envir}
  export gespath=$PTMP/$LOGNAME/nwges/${envir}
# export gespath=/nwges/${envir}
# export GESNDAS=/ptmpp1/$LOGNAME/nwges/prod
  export GESNAM=/gpfs/dell1/nco/ops/nwges/prod
  export GESNDAS=/gpfs/dell1/nco/ops/nwges/prod

# com
# export COMGFS=/gpfs/hps/nco/ops/com/gfs/prod
  export COMGFS=/gpfs/dell1/nco/ops/com/gfs/prod
  export COMINgfs=/gpfs/dell1/nco/ops/com/gfs/prod
  export COMENS=/gpfs/${machine}/nco/ops/com/gens/prod
  export COMINgens=/gpfs/${machine}/nco/ops/com/gens/prod

# export COMRTMA=/gpfs/${machine}/nco/ops/com/rtma/prod
  export COMRTMA=/gpfs/${machine}/nco/ops/com/rtma/prod
