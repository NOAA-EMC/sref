SHELL=/bin/sh
SRCS 	= clusterweight.f
OBJS	= clusterweight.o

FC 	= ftn -O3
#FC 	= ftn -O3 -mcmodel=medium
#FC 	= ftn -O3 -convert big_endian
LDFLAGS =
LIBS 	= ${W3NCO_LIB4} ${W3EMC_LIB4} ${BACIO_LIB4}
CMD 	= sref_clusterweight

all:	$(CMD)

$(CMD):	 ${OBJS}
	$(FC) $(LDFLAGS) -o $(@) $(OBJS) $(LIBS)
