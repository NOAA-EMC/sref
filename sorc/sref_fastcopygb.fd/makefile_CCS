# This makefile was produced by fmjif at 17:21:11 on 97/02/24.
SHELL=	/bin/sh
CMD=	fastcopygb
FOPTIP= -O3 -qsmp=noauto -qnosave -qintsize=4 -qrealsize=8
FOPTW3=	-O3 -qnosave -qrealsize=8 -qmoddir=/nwprod/lib/incmod/w3_d
FOPTS=	-O3   -qrealsize=8 -qnosave
LOPTS=	-qsmp=noauto     -lessl
INCS=	
OBJS=	copygb.o sv.polates0.o sv.polatev0.o sv.polates1.o sv.polatev1.o sv.polates2.o sv.polatev2.o putgben.o
LIBS= -L/nwprod/lib -lw3_d -lbacio_4 -lip_d -lsp_d	
################################################################################
$(CMD):	$(OBJS)
	ncepxlf $(LOPTS) $(OBJS) $(LIBS) -o $(CMD)
$(OBJS):	$(INCS)
sv.polates0.o:	sv.polates0.f
	ncepxlf -qsmp=noauto -c $(FOPTIP) $*.f
sv.polatev0.o:	sv.polatev0.f
	ncepxlf -qsmp=noauto -c $(FOPTIP) $*.f
sv.polates1.o:	sv.polates1.f
	ncepxlf -qsmp=noauto -c $(FOPTIP) $*.f
sv.polatev1.o:	sv.polatev1.f
	ncepxlf -qsmp=noauto -c $(FOPTIP) $*.f
sv.polates2.o:	sv.polates2.f
	ncepxlf -qsmp=noauto -c $(FOPTIP) $*.f
sv.polatev2.o:	sv.polatev2.f
	ncepxlf -qsmp=noauto -c $(FOPTIP) $*.f
putgben.o:	putgben.f
	ncepxlf -c $(FOPTW3) $*.f
copygb.o:	copygb.f
	ncepxlf -c $(FOPTS) $*.f
clean:	
	-rm -f $(OBJS)
	ncepxlf -c \$(FFLAGS) \$<
